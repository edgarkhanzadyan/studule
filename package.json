{
  "name": "studule",
  "engines": {
    "node": ">=6.7.0"
  },
  "version": "1.0.0",
  "description": "schedule for students",
  "main": "index.js",
  "scripts": {
    "backend": "babel --presets=react --plugins=transform-class-properties lib/schedule.jsx --out-file lib/schedule.js",
    "build": "webpack --progress",
    "build-site": "npm run backend && npm run build"
  },
  "author": "Edgar Khanzadian edgar.khanzadian@gmail.com and Erik Davtyan erikdavtyan@gmail.com",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.3"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "transform-runtime"
    ]
